<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Information About Pokemons</title>
<body>
    <h1>Pokemons</h1>
    <h2>Information About Pokemons</h2>
    <table class="table">
        <thead>
          <tr>
            <th scope="col">id</th>
            <th scope="col">name</th>
            <th scope="col">height</th>
            <th scope="col">weight</th>
            <th scope="col">image</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <th scope="row"><%= pokemon_one.id %></th>
            <td><%= pokemon_one.name %></td>
            <td><%= pokemon_one.height %></td>
            <td><%= pokemon_one.weight %></td>
            <td>    <img width="50px" src="https://i.pinimg.com/originals/dc/ab/b7/dcabb7fbb2f763d680d20a3d228cc6f9.jpg"></td>
          </tr>
          <tr>
            <th scope="row"><%= pokemon_two.id %></th>
            <td><%= pokemon_two.name %></td>
            <td><%= pokemon_two.height %></td>
            <td><%= pokemon_two.weight %></td>
            <td>    <img width="50px" src="https://assets.pokemon.com/assets/cms2/img/pokedex/full/133.png"></td>          </tr>
          <tr>
            <th scope="row"><%= pokemon_three.id %></th>
            <td><%= pokemon_three.name %></td>
            <td><%= pokemon_three.height %></td>
            <td><%= pokemon_three.weight %></td>
            <td>    <img width="50px" src="https://assets.pokemon.com/assets/cms2/img/pokedex/full/007.png"></td>          </tr>
        </tbody>
      </table>
</body>
</html>